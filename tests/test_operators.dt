// Test operators and expressions
package test;

fn test_operators() void {
    var a = 10;
    var b = 5;
    
    // Arithmetic operators
    var sum = a + b;
    var diff = a - b;
    var product = a * b;
    var quotient = a / b;
    var remainder = a % b;
    
    // Assignment operators
    a += 5;
    b -= 2;
    a *= 3;
    b /= 2;
    a %= 7;
    
    // Comparison operators
    var equal = (a == b);
    var not_equal = (a != b);
    var less = (a < b);
    var less_equal = (a <= b);
    var greater = (a > b);
    var greater_equal = (a >= b);
    
    // Logical operators
    var and_result = (true && false);
    var or_result = (true || false);
    var not_result = !true;
    
    return;
}